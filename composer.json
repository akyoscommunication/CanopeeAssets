{
    "name": "akyos/canopee-assets",
    "type": "assets",
    "description": "Symfony Canopee Assets",
    "license": "MIT",
    "authors": [
	{
		"name": "Lilian Carneau",
		"email": "lilian@akyos.com"
	},
	{
		"name": "Thomas Sebert",
		"email": "thomas@akyos.com"
	},
	{
		"name": "Akyos Communication",
		"homepage": "https://akyos.com"
	}
    ],
    "keywords": ["assets", "canopee"],
    "require": {
      "php": ">=8.2",
      "symfony/twig-bundle": "6.3.*",
      "symfony/ux-autocomplete": "*",
      "symfony/ux-dropzone": "^2.12",
      "symfony/ux-live-component": "^2.12",
      "symfony/ux-react": "^2.12",
      "symfony/ux-toggle-password": "^2.12",
      "symfony/ux-translator": "^2.13",
      "symfony/ux-twig-component": "^2.12"
    },
    "autoload": {
        "psr-4": { "Akyos\\CanopeeAssets\\": "src/" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    }
}
